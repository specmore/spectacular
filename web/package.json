{
  "name": "spectacular",
  "version": "0.1.0",
  "description": "A spectacular tool for collaborating on interface specifications.",
  "scripts": {
    "start-deps": "docker-compose -f ../docker-compose.yml -f ../docker-compose.dev.yml --env-file ../.env up backend",
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "test": "jest",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit",
    "lint": "eslint src/ --ext .js,.ts,.tsx",
    "generate-fetcher": "restful-react import --file ../backend/specs/backend-api.yaml --output src/backend-api-client.tsx",
    "postinstall": "semantic-ui-css-patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/specmore/spectacular.git"
  },
  "author": "Patrick Burls",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/specmore/spectacular/issues"
  },
  "homepage": "https://github.com/specmore/spectacular#readme",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@semantic-ui-react/css-patch": "^1.1.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.14.32",
    "@types/react-dom": "^16.9.16",
    "@types/react-router-dom": "^5.3.3",
    "@types/swagger-ui-react": "^3.35.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "dotenv": "^8.6.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-junit": "^11.1.0",
    "less": "^3.13.1",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "fomantic-ui-less": "^2.8.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nord": "^0.2.1",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-moment": "^1.1.2",
    "react-router": "^5.3.3",
    "react-router-dom": "^5.3.3",
    "restful-react": "^15.9.4",
    "semantic-ui-react": "^2.1.3",
    "swagger-ui-react": "^4.18.2",
    "use-query-params": "^1.2.3"
  },
  "jest-junit": {
    "suiteName": "web jest tests",
    "outputDirectory": "out/results/"
  }
}
